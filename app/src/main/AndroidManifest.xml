<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="A1"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project">
        <activity
            android:name=".activity_wardendash"
            android:label="Warden Dashboard"
            android:parentActivityName=".activity_wardenlogin" />
        <activity
            android:name=".activity_studentdash"
            android:label="Student Dashboard"
            android:parentActivityName=".activity_studentlogin" />
        <activity
            android:name=".activity_application"
            android:label="Register Page"
            android:parentActivityName=".activity_knowmore" />
        <activity
            android:name=".activity_knowmore"
            android:label="Know more Page"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity_wardenlogin"
            android:label="Warden Login Page"
            android:parentActivityName=".activity_login" />
        <activity
            android:name=".activity_studentlogin"
            android:label="Student Login Page"
            android:parentActivityName=".activity_login" />
        <activity
            android:name=".activity_login"
            android:label="Login Page"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>

